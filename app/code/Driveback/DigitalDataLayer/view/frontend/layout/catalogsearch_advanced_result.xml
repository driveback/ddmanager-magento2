<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="driveback_ddl">
            <referenceBlock name="driveback_ddl">
                <action method="setRegistryValue">
                    <argument name="key" xsi:type="string">listing_list_name</argument>
                    <argument name="value" xsi:type="string">type</argument>
                </action>
                <action method="setRegistryValue">
                    <argument name="key" xsi:type="string">listing_product_list_block_name</argument>
                    <argument name="value" xsi:type="string">search_result_list</argument>
                </action>
            </referenceBlock>
        </referenceBlock>
    </body>
</page>
